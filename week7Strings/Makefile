.PHONY: clean All

All:
	@echo "----------Building project:[ reverseFunction - Debug ]----------"
	@cd "reverseFunction" && "$(MAKE)" -f  "reverseFunction.mk"
clean:
	@echo "----------Cleaning project:[ reverseFunction - Debug ]----------"
	@cd "reverseFunction" && "$(MAKE)" -f  "reverseFunction.mk" clean
