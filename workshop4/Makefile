.PHONY: clean All

All:
	@echo "----------Building project:[ Q4_perfectNumbers - Debug ]----------"
	@cd "Q4_perfectNumbers" && "$(MAKE)" -f  "Q4_perfectNumbers.mk"
clean:
	@echo "----------Cleaning project:[ Q4_perfectNumbers - Debug ]----------"
	@cd "Q4_perfectNumbers" && "$(MAKE)" -f  "Q4_perfectNumbers.mk" clean
