.PHONY: clean All

All:
	@echo "----------Building project:[ Q9_sum2Darrays - Debug ]----------"
	@cd "Q9_sum2Darrays" && "$(MAKE)" -f  "Q9_sum2Darrays.mk"
clean:
	@echo "----------Cleaning project:[ Q9_sum2Darrays - Debug ]----------"
	@cd "Q9_sum2Darrays" && "$(MAKE)" -f  "Q9_sum2Darrays.mk" clean
