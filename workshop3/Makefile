.PHONY: clean All

All:
	@echo "----------Building project:[ Q5_largestNumber - Debug ]----------"
	@cd "Q5_largestNumber" && "$(MAKE)" -f  "Q5_largestNumber.mk"
clean:
	@echo "----------Cleaning project:[ Q5_largestNumber - Debug ]----------"
	@cd "Q5_largestNumber" && "$(MAKE)" -f  "Q5_largestNumber.mk" clean
