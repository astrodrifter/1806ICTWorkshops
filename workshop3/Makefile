.PHONY: clean All

All:
	@echo "----------Building project:[ Q6_2LargestNumbers - Debug ]----------"
	@cd "Q6_2LargestNumbers" && "$(MAKE)" -f  "Q6_2LargestNumbers.mk"
clean:
	@echo "----------Cleaning project:[ Q6_2LargestNumbers - Debug ]----------"
	@cd "Q6_2LargestNumbers" && "$(MAKE)" -f  "Q6_2LargestNumbers.mk" clean
