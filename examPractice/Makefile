.PHONY: clean All

All:
	@echo "----------Building project:[ tempConversion - Debug ]----------"
	@cd "tempConversion" && "$(MAKE)" -f  "tempConversion.mk"
clean:
	@echo "----------Cleaning project:[ tempConversion - Debug ]----------"
	@cd "tempConversion" && "$(MAKE)" -f  "tempConversion.mk" clean
