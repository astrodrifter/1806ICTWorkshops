.PHONY: clean All

All:
	@echo "----------Building project:[ reverseSenetance - Debug ]----------"
	@cd "reverseSenetance" && "$(MAKE)" -f  "reverseSenetance.mk"
clean:
	@echo "----------Cleaning project:[ reverseSenetance - Debug ]----------"
	@cd "reverseSenetance" && "$(MAKE)" -f  "reverseSenetance.mk" clean
